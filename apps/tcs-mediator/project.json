{
    "root": "apps/tcs-mediator",
    "sourceRoot": "apps/tcs-mediator/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go build -o=../../dist/apps/tcs-mediator/ ./src",
                "cwd": "apps/tcs-mediator"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "go run ./src",
                "cwd": "apps/tcs-mediator"
            }
        }
    }
}
